# MariaDB Extractor Configuration
# Copy this file to .env and update with your actual values

# Production/External Database Connection
MARIADB_HOST=your-production-db-host
MARIADB_PORT=3306
MARIADB_USER=your_username
MARIADB_PASSWORD=your_password
MARIADB_OUTPUT_PREFIX=mariadb-extract

# Local Development Database (for docker-compose)
MYSQL_ROOT_PASSWORD=password
MYSQL_DATABASE=ecommerce
MYSQL_USER=devuser
MYSQL_PASSWORD=devpass
MARIADB_LOCAL_PORT=3307
ADMINER_PORT=8080